mixin eight_expert_block(eight_expert_photo, eight_h5, eight_h6, eight_p  )
  div.expert_block
    img(class="expert_photo" src=eight_expert_photo alt="expert photo")
    div.text_about_expert
      h5 #{eight_h5}
      hr
      h6 #{eight_h6}
      p #{eight_p}
      hr
      div.social_icons
        a(href="#")
          i(class="fab fa-twitter")
        a(href="#")
          i(class="fab fa-linkedin")
        a(href="#")
          i(class="fab fa-facebook")
        a(href="#")
          i(class="fab fa-skype")
        a(href="#")
          i(class="fab fa-pinterest")
        a(href="#")
          i(class="fab fa-apple")
      
        
